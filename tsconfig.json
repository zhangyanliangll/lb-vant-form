{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "examples/**/*",
    "examples/**/*.vue",
    "packages/**/*",
    "packages/**/*.vue",
    "vite-env.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "unplugin-vue-define-options/macros-global"
    ],
    "paths": {
      "@/*": [
        "./examples/*"
      ]
    }
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ],
}
